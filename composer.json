{
    "name": "jbelien/mapfile-generator",
    "description": "MapServer .MAP Generator (Read & Write)",
    "type": "project",
    "license": "GPL-2.0",
    "keywords": [],
    "config": {
        "sort-packages": true
    },
    "extra": {
        "zf": {
            "component-whitelist": [
                "zendframework/zend-expressive",
                "zendframework/zend-expressive-helpers",
                "zendframework/zend-expressive-router",
                "zendframework/zend-httphandlerrunner",
                "zendframework/zend-expressive-fastroute",
                "zendframework/zend-expressive-twigrenderer"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ext-mbstring": "*",
        "zendframework/zend-component-installer": "2.1.2",
        "zendframework/zend-config-aggregator": "1.1.1",
        "zendframework/zend-diactoros": "2.1.3",
        "zendframework/zend-expressive": "3.2.1",
        "zendframework/zend-expressive-fastroute": "3.0.3",
        "zendframework/zend-expressive-helpers": "5.3.0",
        "zendframework/zend-expressive-session-ext": "1.7.0",
        "zendframework/zend-expressive-twigrenderer": "2.4.0",
        "zendframework/zend-servicemanager": "3.4.0",
        "zendframework/zend-stdlib": "3.2.1",
        "jbelien/mapfile-php-library": "v2.0.6-beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0.4",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.3.2",
        "zendframework/zend-expressive-tooling": "^1.0",
        "zfcampus/zf-development-mode": "^3.1",
        "filp/whoops": "^2.1.12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@development-enable",
            "npm install"
        ],
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "expressive": "expressive --ansi",
        "check": [
            "@cs-check",
            "@test",
            "@analyze"
        ],
        "analyze": "phpstan analyze -l max -c ./phpstan.installer.neon ./src ./config",
        "clear-config-cache": "php bin/clear-config-cache.php",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "serve": "php -S 0.0.0.0:8080 -t public/ -d display_error=1",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml"
    }
}
